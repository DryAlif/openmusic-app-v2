{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": "off",
    "no-console": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "camelcase": "off",
    "class-methods-use-this": "off",
    "consistent-return": "off"

    // "linebreak-style": 0
  }
}
